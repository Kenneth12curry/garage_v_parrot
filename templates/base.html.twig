  <body>

    <!-- nav -->
	  <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
	    <div class="container">

            <a class="navbar-brand .personnalisation" href="{{path('app_home')}}">Garage<span class="text-danger">&nbsp; V.Parrot</span></a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">

                    <li class="nav-item {% if app.request.attributes.get('_route') == 'app_home' or app.request.attributes.get('_route') == 'home' %} active {% endif %}">
                        <a href="{{path('app_home')}}" class="nav-link {% if app.request.attributes.get('_route') == 'app_home' or app.request.attributes.get('_route') == 'home' %}text-danger{% endif %}">
                            <i class="fa fa-home opacity-6 text-white"></i>
                            <span class="me-2">Home</span>
                        </a>
                    </li>
                    <li class="nav-item {% if app.request.attributes.get('_route') == 'app_service' %} active {% endif %}">
                        <a href="{{path('app_service')}}" class="nav-link {% if app.request.attributes.get('_route') == 'app_service' %}text-danger{% endif %}">
                            <i class="fa fa-cogs opacity-6 text-white"></i>
                            <span class="me-2">Services</span>
                        </a>
                    </li>
                    <li class="nav-item {% if app.request.attributes.get('_route') == 'app_cars' %}active{% endif %}">
                        <a href="{{path('app_cars')}}" class="nav-link {% if app.request.attributes.get('_route') == 'app_cars' %} text-danger  {% endif %}">
                            <i class="fa fa-car opacity-6 text-white"></i>
                            <span class="me-2">Cars</span>
                        </a>
                    </li>
                    <li class="nav-item {% if app.request.attributes.get('_route') == 'app_contact' %}active{% endif %}">
                        <a href="{{path('app_contact')}}" class="nav-link {% if app.request.attributes.get('_route') == 'app_contact' %}text-danger{% endif %}">
                            <i class="fa fa-pencil opacity-6 text-white"></i>
                            <span class="me-2">Contact</span>
                        </a>
                    </li>
                    <li class="nav-item {% if app.request.attributes.get('_route') == 'app_add_notice' %}active{% endif %}">
                        <a href="{{path('app_add_notice')}}" class="nav-link {% if app.request.attributes.get('_route') == 'app_add_notice' %}text-danger{% endif %}">
                            <i class="fas fa-star opacity-6 text-white"></i>
                            <span class="me-2">Notice</span>
                        </a>
                  </li>
                  <li class="nav-item">
                      <a class="text-white nav-link" href="{{path('app_login')}}">
                          <i class="fa fa-sign-in-alt opacity-6 text-white"></i>
                          <span class="me-2 fw-bold">To Login</span>
                      </a>
                  </li>
                </ul>
            </div>
	    </div>
	  </nav>
